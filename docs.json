{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Parcel Tracer",
  "colors": {
    "primary": "#71459A",
    "light": "#71459A",
    "dark": "#1a1625"
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "pages": [
          "index"
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "api-reference/quickstart"
            ]
          },
          {
            "group": "Models",
            "pages": [
              "models/customers",
              "models/areas",
              "models/customer-locations",
              "models/orders"
            ]
          },
          {
            "group": "Endpoints",
            "openapi": {
              "source": "/api-reference/schema.yaml"
            }
          }
        ]
      },
      {
        "tab": "Webhooks",
        "pages": [
          "webhooks/getting-started",
          "webhooks/securing-webhooks",
          "webhooks/monitoring"
        ]
      },
      {
        "tab": "Privacy Policy",
        "pages": [
          "privacy-policy"
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/main-logo.png",
    "dark": "/logo/main-logo.png"
  },
  "navbar": {
    "style": "default",
    "links": [
      {
        "label": "Contact Us",
        "href": "mailto:info@parceltracer.app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Open App",
      "href": "https://prod.parceltracer.app"
    }
  }
}